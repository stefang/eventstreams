- unless current_subdomain.blank?
  - title @talk.title

  %p
    %strong Title:
    =h @talk.title
  %p
    %strong Description:
    =h @talk.description
  %p
    %strong Start:
    =h @talk.start
  %p
    %strong Duration:
    =h @talk.duration
  %p
    %strong Track:
    =h @talk.track_id
  %p
    %strong Published:
    =h @talk.published

-else 
  - title @talk.title

  %p
    %strong Title:
    =h @talk.title
  %p
    %strong Description:
    =h @talk.description
  %p
    %strong Start:
    =h @talk.start
  %p
    %strong Duration:
    =h @talk.duration
  %p
    %strong Track:
    =h @talk.track_id
  %p
    %strong Published:
    =h @talk.published

  %p
    = link_to "Edit", edit_user_event_talk_path(current_user, @event, @talk)
    |
    = link_to "Destroy", user_event_talk_path(current_user, @event, @talk), :confirm => 'Are you sure?', :method => :delete
    |
    = link_to "View All", user_event_talks_path
