section#eventstreams_event header .inner #event_logo {
  margin-bottom: 6px; }
section#eventstreams_event header .inner #event_logo a {
  display: block;
  font-family: KomikaAxisRegular, sans-serif;
  font-weight: normal;
  font-size: 40px;
  line-height: 41px;
  text-transform: uppercase;
  color: #333333;
  text-shadow: 1px 1px 0px white, 4px 4px 0px rgba(0, 0, 0, 0.2); 
  margin: -6px 0 11px 0;}
section#eventstreams_event header .inner #event_logo a:hover, section#eventstreams_event header .inner #event_logo a:focus {
  color: #555555; }
section#eventstreams_event header .inner .tagline {
  margin: 0 0 3px 0;
  color: #888888;
  line-height: 22px;
  text-transform: uppercase;
  font-family: KomikaAxisRegular, sans-serif;
  font-weight: normal;
  font-size: 13px; }
section#eventstreams_event header .inner .event_meta {
  position: absolute;
  right: 20px;
  top: 63px;
  text-align: right;
  text-transform: uppercase;
  font-family: KomikaAxisRegular, sans-serif;
  font-weight: normal;
  font-size: 14px; }
section#eventstreams_event header .inner .event_meta li {
  margin-bottom: 3px; }
section#eventstreams_event header .inner .event_meta li abbr {
  display: block;
  color: #aaaaaa; }

section#eventstreams_event section.main .speaker h4,  
section#eventstreams_event section.main section.event_venues h4, 
section#eventstreams_event section.main section.other_venues h4,
section#eventstreams_event ul.speakers li.speaker h4,
section.tweets h4 {
  line-height: 22px;
  text-transform: uppercase;
  font-family: KomikaAxisRegular, sans-serif;
  font-weight: normal;
  font-size: 19px;
  margin-top: -4px;
  padding-bottom: 4px;
}

section#eventstreams_event section.main section.event_venues h4, 
section#eventstreams_event section.main section.other_venues h4 {
  padding-bottom: 24px;
  padding-top:2px;
}

section#eventstreams_event section.main .speaker h4,
section#eventstreams_event ul.speakers li.speaker h4 {
  margin-left: 120px
}

section#eventstreams_event article.talk h1.talk_title {
  line-height: 22px;
  text-transform: uppercase;
  font-family: KomikaAxisRegular, sans-serif;
  font-weight: normal;
  font-size: 20px;
  margin-top: -4px;
  padding-bottom: 4px;
}

section#eventstreams_event article.talk h1.talk_title span {
  font-size: 12px;
  bottom: -1px;
}