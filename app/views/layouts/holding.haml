!!!
%html{:xmlns => "http://www.w3.org/1999/xhtml", "xml:lang" => "en", :lang => "en"}
  %head
    %title= yield(:title) || t('title')
    %meta{:name=>'viewport', :content=>'width=700px'}
    %link{:rel=>'shortcut icon', :href => image_path("favicon.ico")}
    %link{:rel=>'icon', :type=>'image/png', :href => image_path("favicon.png")}
    = javascript_include_tag "jquery", "jquery-notice", "holding", :cache => false
    = stylesheet_link_tag 'holding',:media => 'screen, projection', :cache => false
    -if RAILS_ENV == "production"
      = render :partial => 'layouts/ga'  
  %body
    = yield
    = render :partial => 'layouts/flashes'